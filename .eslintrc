{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style": 0,
    "require-jsdoc" : 0,
    "eol-last": 0,
    "padded-blocks": 0,
    "no-trailing-spaces": 0,
    "comma-dangle": 0,
    "no-unused-vars": 0,
    "operator-linebreak": 0
  },
  
  "extends": ["eslint:recommended", "google"]
}